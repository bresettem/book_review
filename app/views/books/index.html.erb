<div class="container">
	<div class="row">
		<%#= render 'carousel' %>
		<h1><%= number_with_delimiter(@pages_count) %> pages have been added</h1>
	</div>
</div>
<div class="container">
	<div class="row">
		<% @allBooks.each do |a| %>
			<%= link_to (image_tag a.image_link, class: 'img-thumbnail'), book_path(a) %>
		<% end %>
	</div>
</div>